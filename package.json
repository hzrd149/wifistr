    "@solidjs/router": "^0.15.3",
    "applesauce-accounts": "^4",
    "applesauce-actions": "^4",
    "applesauce-core": "^4",
    "applesauce-factory": "^4",
    "applesauce-loaders": "^4",
    "applesauce-relay": "^4",
    "applesauce-signers": "^4",
    "fuse.js": "^7.1.0",
    "leaflet": "^1.9.4",
    "leaflet.locatecontrol": "^0.83.1",
