{
  "name": "wifistr",
  "description": "open wifi map powered by nostr",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "tsc -b && vite build && node scripts/build-index.js",
    "preview": "vite preview",
